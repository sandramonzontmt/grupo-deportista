<?php

namespace EventosBundle\Entity;

use Doctrine\Common\Collections\ArrayCollection;

class Evento
{
	private $id;
	protected $name;
	protected $description;
	protected $sport;
    protected $organizator;
	protected $asssistants;	
	/*protected $price;
	protected $material;
	protected $date_start;
	protected $date_end;*/


    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set name
     *
     * @param string $name
     *
     * @return Evento
     */
    public function setName($name)
    {
        $this->name = $name;

        return $this;
    }

    /**
     * Get name
     *
     * @return string
     */
    public function getName()
    {
        return $this->name;
    }

    /**
     * Set description
     *
     * @param string $description
     *
     * @return Evento
     */
    public function setDescription($description)
    {
        $this->description = $description;

        return $this;
    }

    /**
     * Get description
     *
     * @return string
     */
    public function getDescription()
    {
        return $this->description;
    }

    /**
     * Set sport
     *
     * @param string $sport
     *
     * @return Evento
     */
    public function setSport($sport)
    {
        $this->sport = $sport;

        return $this;
    }

    /**
     * Get sport
     *
     * @return string
     */
    public function getSport()
    {
        return $this->sport;
    }
}
